{
    "name": "expirychecker",
    "version": "1.0.0",
    "main": "index.js",
    "license": "MIT",
    "private": "true",
    "workspaces": {
        "packages": [
            "packages/expirychecker",
            "packages/teams",
            "packages/shared"
        ],
